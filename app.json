{
  "expo": {
    "name": "PetFurMe",
    "slug": "PetFurMe",
    "version": "1.0.0",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "NOTIFICATIONS",
        "VIBRATE"
      ],
      "package": "com.petfurme",
      "versionCode": 1,
      "softwareKeyboardLayoutMode": "pan",
      "enableHermes": true
    },
    "web": {
      "favicon": "./src/assets/favicon.png",
      "bundler": "metro",
      "build": {
        "babel": {
          "include": [
            "@expo/vector-icons",
            "react-native-gesture-handler"
          ]
        }
      }
    },
    "plugins": [
      "expo-notifications",
      "expo-font"
    ],
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps"
        }
      ]
    }
  }
}
