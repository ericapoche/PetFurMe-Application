{
  "expo": {
    "name": "PetFurMe",
    "slug": "PetFurMe",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.petfurme.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.petfurme.app"
    },
    "web": {
      "favicon": "./src/assets/images/favicon.png"
    },
    "newArchEnabled": true,
    "infoPlist": {
      "UIBackgroundModes": [
        "remote-notification"
      ]
    },
    "permissions": [
      "NOTIFICATIONS",
      "VIBRATE"
    ],
    "versionCode": 1,
    "softwareKeyboardLayoutMode": "pan",
    "enableHermes": true,
    "bundler": "metro",
    "build": {
      "babel": {
        "include": [
          "@expo/vector-icons",
          "react-native-gesture-handler"
        ]
      }
    },
    "plugins": [
      "expo-notifications",
      "expo-font"
    ],
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps"
        }
      ]
    }
  }
}
